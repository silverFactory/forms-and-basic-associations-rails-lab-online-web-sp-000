<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <input type="text" name="[notes][1]" id="song_notes_1">
  <input type="text" name="[notes][2]" id="song_notes_2">
  <input type="text" name="[notes][3]" id="song_notes_3">
  <%= f.submit %>
<% end %>
